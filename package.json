{
  "name": "lar",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns=e2e-spec.ts --testPathIgnorePatterns=service.spec.ts --testPathIgnorePatterns=repository.spec.ts",
    "test:repository": "jest repository.spec.ts",
    "test:integration": "jest service.spec.ts",
    "test:e2e": "jest e2e-spec.ts --runInBand",
    "test:all": "npm run test:unit && npm run test:repository && npm run test:integration && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "commit": "cz",
    "rimraf": "rimraf",
    "typeorm": "ts-node --files -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:ds": "npm run typeorm -- -d src/@core/database/config/data-source.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:generate": "npm run typeorm:ds -- migration:generate",
    "migration:run": "npm run typeorm:ds -- migration:run",
    "migration:revert": "npm run typeorm:ds -- migration:revert",
    "migration:show": "npm run typeorm:ds -- migration:show",
    "migration:drop": "npm run typeorm:ds -- schema:drop",
    "seed:run": "ts-node --files -r tsconfig-paths/register src/@core/database/seeds/run-seeds.ts",
    "seed:create": "ts-node --files -r tsconfig-paths/register ./node_modules/typeorm-extension/dist/cli/index.js seed:create -n",
    "db:fresh": "npm run migration:drop && npm run rimraf -- src/@core/database/migrations/* && npm run migration:generate -- src/@core/database/migrations/InitialMigration && npm run migration:run && npm run seed:run",
    "db:setup": "npm run migration:run && npm run seed:run",
    "email:test": "ts-node --files -r tsconfig-paths/register src/@core/email/email.test.ts",
    "email:template:create": "ts-node --files -r tsconfig-paths/register src/@core/email/scripts/create-template.ts",
    "logger:test": "ts-node --files -r tsconfig-paths/register src/@core/logger/logger.test.ts",
    "files:test": "ts-node --files -r tsconfig-paths/register src/@core/files/files.test.ts",
    "files:verify": "ts-node --files -r tsconfig-paths/register src/@core/files/verify-setup.ts",
    "i18n:test": "ts-node --files -r tsconfig-paths/register src/@core/i18n/i18n.test.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:ps": "docker compose ps",
    "docker:verify": "./scripts/verify-docker.sh"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.10",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.10",
    "bcrypt": "^6.0.0",
    "casbin": "^5.48.0",
    "chalk": "^5.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "googleapis": "^169.0.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.9.1",
    "multer": "^2.0.2",
    "nestjs-cls": "^6.1.0",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.1.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.3",
    "sql-formatter": "^15.6.12",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.28",
    "typeorm-adapter": "^1.8.0",
    "typeorm-extension": "^3.7.3",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^7.0.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "clearMocks": true,
    "moduleNameMapper": {
      "^@core/(.*)$": "<rootDir>/@core/$1",
      "^@core$": "<rootDir>/@core",
      "^@shared/(.*)$": "<rootDir>/@shared/$1",
      "^@shared$": "<rootDir>/@shared"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(chalk|#ansi-styles|#supports-color|uuid)/)"
    ]
  }
}
